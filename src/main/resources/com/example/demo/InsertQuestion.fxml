<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="880.0" style="-fx-background-color: #4ed4d4;" stylesheets="@menugame.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.Game.InsertQuestion">
   <children>
      <Label layoutX="214.0" layoutY="165.0" prefHeight="21.0" prefWidth="88.0" text="Question" textFill="#262827">
         <font>
            <Font name="Times New Roman Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="214.0" layoutY="264.0" prefHeight="23.0" prefWidth="88.0" text="Correct" textFill="#202321">
         <font>
            <Font name="Times New Roman Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="Question" layoutX="329.0" layoutY="149.0" prefHeight="54.0" prefWidth="356.0" styleClass="rounded-button">
         <stylesheets>
            <URL value="@save.css" />
            <URL value="@menugame.css" />
         </stylesheets></TextField>
      <TextField fx:id="Correct" layoutX="329.0" layoutY="248.0" prefHeight="54.0" prefWidth="356.0" styleClass="rounded-button" stylesheets="@save.css" />
      <Button layoutX="524.0" layoutY="390.0" mnemonicParsing="false" onAction="#Insert" prefHeight="30.0" prefWidth="52.0" style="-fx-background-color: #ffffff;" styleClass="active" stylesheets="@save.css" text="OK">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="buttonBack" layoutX="302.0" layoutY="390.0" mnemonicParsing="false" style="-fx-background-color: #ffffff;" styleClass="active" stylesheets="@save.css" text="Back">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
